<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.robin.angrynerds_wip">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".OverviewActivity"></activity>

        <activity android:name=".activities.note.note.Init"
            android:windowSoftInputMode="adjustPan">
        </activity>
        <activity android:name=".activities.note.tageditor.TagActivity"
            android:windowSoftInputMode="adjustPan">
        </activity>
        <activity android:name=".activities.event.Init"></activity>
        <activity android:name=".overview.overviewActivity.OverviewInit"
            android:configChanges="orientation"></activity>
    </application>

</manifest>