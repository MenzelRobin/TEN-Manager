<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout
    android:id="@+id/id_main_relativeLayout_main"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/id_main_imageView_image"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        tools:srcCompat="@tools:sample/avatars" />


    <LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">





        <LinearLayout
            android:id="@+id/id_main_linearLayout_skip"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_weight="1"
            android:onClick="onClickSkip"
            tools:layout_editor_absoluteY="488dp">
        </LinearLayout>

            <LinearLayout
            android:id="@+id/id_main_linearLayout_reset"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_weight="4"
            android:onClick="onClickReset"
            tools:layout_editor_absoluteY="585dp">
            </LinearLayout>




</LinearLayout>
</RelativeLayout>